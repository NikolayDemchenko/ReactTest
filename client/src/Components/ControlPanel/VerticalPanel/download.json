{
  "height": "916px",
  "width": "1600px",
  "backgroundColor": "#172e46",
  "margin": "10px",
  "marginBottom": "10px",
  "&:hover": {
    "transform": "perspective(200px) scaleZ(-0.5) translateZ(-2px)",
    "boxShadow": "0 8px 5px 2px #0005, inset 0 8px 5px 2px #00ffff",
    "transition": "0.2s"
  },
  "&:active": { "transition": "0s", "boxShadow": "none" }
}
